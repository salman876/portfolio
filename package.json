{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && tsc -b && vite build",
    "build-cf": "vite build",
    "checktypes": "tsc --noEmit",
    "checkimports": "ts-unused-exports tsconfig.json",
    "format": "prettier -w src/**/*.{js,ts,jsx,tsx} -c --no-error-on-unmatched-pattern",
    "format:ci": "prettier -c src/**/*.{js,ts,jsx,tsx} -c --no-error-on-unmatched-pattern",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "eslint:ci": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run --config ./vitest.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.7",
    "lightweight-charts": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.10.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.12.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^8.57.x",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-shopify-lean": "^2.2.0",
    "eslint-plugin-styled-components-a11y": "^2.1.34",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.5"
  },
  "main": "index.js",
  "repository": "git@github.com:salman876/portfolio.git",
  "author": "Salman <salman.876@outlook.com>",
  "license": "MIT"
}
